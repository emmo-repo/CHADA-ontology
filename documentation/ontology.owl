        <emmo:EMMO_967080e5_2f42_4eb2_a3a9_c58143e835f9 xml:lang="en">Hardware used for the preparation of the sample.</emmo:EMMO_967080e5_2f42_4eb2_a3a9_c58143e835f9>
        <rdfs:comment></rdfs:comment>
        <skos:prefLabel xml:lang="en">SamplePreparationHardware</skos:prefLabel>
    </owl:Class>
    


